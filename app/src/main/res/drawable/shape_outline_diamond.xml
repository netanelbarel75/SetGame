<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="45"
    android:toDegrees="45"
    android:pivotX="50%"
    android:pivotY="50%">
    <shape android:shape="rectangle">
        <size
            android:width="35dp"
            android:height="35dp" />
        <stroke
            android:width="2dp"
            android:color="#00FF00" />
    </shape>
</rotate>